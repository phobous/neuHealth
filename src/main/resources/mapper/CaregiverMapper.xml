<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.neuhealth.demo.mapper.CaregiverMapper">

    <!-- 根据姓名模糊查询护理人员 -->
    <select id="selectByNameLike" parameterType="String" resultType="com.neuhealth.demo.domain.Caregiver">
        SELECT *
        FROM caregivers
        WHERE name LIKE CONCAT('%', #{name}, '%')
    </select>


</mapper>
